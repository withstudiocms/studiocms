{
  "name": "@studiocms/devapps",
  "version": "0.1.0-beta.7",
  "description": "A dedicated CMS for Astro DB. Built from the ground up by the Astro community.",
  "author": {
    "name": "Adam Matthiesen | Jacob Jenkins | Paul Valladares",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92"
  ],
  "license": "MIT",
  "keywords": [
    "astro",
    "astrodb",
    "astrolicious",
    "astro-integration",
    "studiocms",
    "withastro"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "assets"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "build-scripts build 'src/**/*.ts'",
    "dev": "build-scripts dev 'src/**/*.ts'"
  },
  "type": "module",
  "dependencies": {
    "astro-integration-kit": "catalog:",
    "@libsql/client": "catalog:studiocms-devapps",
    "cheerio": "catalog:studiocms-devapps",
    "@types/cheerio": "catalog:studiocms-devapps",
    "turndown": "catalog:studiocms-devapps",
    "@types/turndown": "catalog:studiocms-devapps",
    "html-entities": "catalog:studiocms-devapps"
  },
  "peerDependencies": {
    "@astrojs/db": "catalog:min",
    "astro": "catalog:min",
    "vite": "catalog:min"
  },
  "devDependencies": {
    "typescript": "catalog:"
  }
}
