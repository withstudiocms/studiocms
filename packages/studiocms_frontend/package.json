{
  "name": "@studiocms/frontend",
  "version": "0.1.0-beta.7",
  "description": "Core component for the StudioCMS Integration",
  "author": {
    "name": "Adam Matthiesen | Jacob Jenkins | Paul Valladares",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92"
  ],
  "license": "MIT",
  "keywords": [
    "astro-studiocms",
    "cms",
    "studiocms"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist", "components", "routes"
  ],
  "scripts": {
    "build": "build-scripts build 'src/**/*.{ts,css}' --build-tsconfig",
    "dev": "build-scripts dev 'src/**/*.{ts,css}' --build-tsconfig"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components/*": "./dist/components/*",
    "./styles/*": "./dist/styles/*"
  },
  "type": "module",
  "dependencies": {
    "@matthiesenxyz/integration-utils": "catalog:studiocms-shared",
    "@studiocms/ui": "catalog:",
    "astro-integration-kit": "catalog:"
  },
  "peerDependencies": {
    "astro": "catalog:min",
    "vite": "catalog:min"
  },
  "devDependencies": {
    "typescript": "catalog:"
  }
}
