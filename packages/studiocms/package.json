{
  "name": "studiocms",
  "version": "0.1.0-beta.7",
  "description": "A dedicated CMS for Astro and Astro DB. Built from the ground up by the Astro community.",
  "author": {
    "name": "Adam Matthiesen | Jacob Jenkins | Paul Valladares",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92"
  ],
  "license": "MIT",
  "keywords": [
    "cms",
    "astrocms",
    "astrodb",
    "astrostudio",
    "astrostudiocms",
    "studiocms",
    "astro-studio",
    "astro-studiocms",
    "astro",
    "withastro",
    "astro-integration"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "files": [
    "dist",
    "static",
    "custom-renderers",
    "renderer.d.ts",
    "auth.d.ts",
    "core.d.ts",
    "ui.d.ts",
    "studiocms-cli.mjs",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "build-scripts build 'src/**/*.{ts,css}'",
    "dev": "build-scripts dev 'src/**/*.{ts,css}'"
  },
  "bin": {
    "studiocms": "./studiocms-cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./consts": {
      "types": "./dist/consts.d.ts",
      "default": "./dist/consts.js"
    },
    "./contentRenderer": {
      "types": "./dist/renderer/contentRenderer.d.ts",
      "default": "./dist/renderer/contentRenderer.js"
    },
    "./Renderer": {
      "types": "./static/components/Renderer.d.ts",
      "default": "./static/components/Renderer.js"
    },
    "./custom-renderers": "./custom-renderers/index.ts",
    "./db/config": {
      "types": "./dist/db/config.d.ts",
      "default": "./dist/db/config.js"
    },
    "./db/tables": {
      "types": "./dist/db/tables.d.ts",
      "default": "./dist/db/tables.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./sdk/base": {
      "types": "./dist/sdk/StudioCMSSDK.d.ts",
      "default": "./dist/sdk/StudioCMSSDK.js"
    },
    "./sdk/cache": {
      "types": "./dist/sdk/StudioCMSVirtualCache.d.ts",
      "default": "./dist/sdk/StudioCMSVirtualCache.js"
    },
    "./sdk/tables": {
      "types": "./dist/sdk/tables.d.ts",
      "default": "./dist/sdk/tables.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "default": "./dist/schemas/index.js"
    },
    "./schemas/renderer": {
      "types": "./dist/schemas/config/rendererConfig.d.ts",
      "default": "./dist/schemas/config/rendererConfig.js"
    },
    "./static/*": "./static/*",
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    }
  },
  "type": "module",
  "dependencies": {
    "@studiocms/markdown-remark-processor": "^1.0.0",
    "@studiocms/ui": "catalog:",

    "astro-integration-kit": "catalog:",
    "@inox-tools/aik-mod": "0.9.1",

    "package-json": "catalog:studiocms",
    "semver": "catalog:studiocms",

    "@markdoc/markdoc": "catalog:studiocms-renderer",
    "@mdx-js/mdx": "catalog:studiocms-renderer",
    "react-dom": "catalog:studiocms-renderer",
    "react": "catalog:studiocms-renderer",
    "remark-gfm": "catalog:studiocms-renderer",
    "rehype-highlight": "catalog:studiocms-renderer",

    "@oslojs/binary": "catalog:studiocms-auth",
    "@oslojs/crypto": "catalog:studiocms-auth",
    "@oslojs/encoding": "catalog:studiocms-auth",
    "@types/three": "catalog:studiocms-auth",
    "arctic": "catalog:studiocms-auth",
    "three": "catalog:studiocms-auth",

    "mrmime": "catalog:studiocms-core",
    "unified": "catalog:studiocms-core",
    "remark-rehype": "catalog:studiocms-core",
    "mdast-util-to-hast": "catalog:studiocms-core",
    "drizzle-orm": "catalog:studiocms-core",
    
    "@cloudinary/url-gen": "catalog:studiocms-imagehandler",

    "@inox-tools/runtime-logger": "catalog:studiocms-shared",
    "@matthiesenxyz/astrodtsbuilder": "catalog:studiocms-shared",
    "@matthiesenxyz/integration-utils": "catalog:studiocms-shared",
    "rollup-plugin-copy": "catalog:studiocms-shared",
    "mdast": "catalog:studiocms-shared",
    "mdast-util-from-markdown": "catalog:studiocms-shared",
    "mdast-util-to-markdown": "catalog:studiocms-shared",
    "mdast-util-to-string": "catalog:studiocms-shared",
    "unist-util-visit": "catalog:studiocms-shared",
    "lodash": "catalog:studiocms-shared",
    "@fontsource-variable/onest": "catalog:studiocms-shared",

    "@bluwy/giget-core": "^0.1.2",
    "@clack/core": "0.4.1",
    "@clack/prompts": "0.9.1",
    "@commander-js/extra-typings": "^13.0.0",
    "commander": "^13.0.0",
    "figlet": "^1.8.0",
    "chalk": "^5.4.1",
    "strip-ansi": "^7.1.0",
    "wrap-ansi": "^9.0.0",
    "boxen": "^8.0.1",
    "is-unicode-supported": "^2.1.0",
    "ansi-escapes": "^7.0.0",
    "cli-cursor": "^5.0.0",
    "slice-ansi": "^7.1.0",
    "ultrahtml": "^1.5.3",
    "jsonwebtoken": "^9.0.2",
    "diff": "^7.0.0",
    "diff2html": "^3.4.51",
    "dompurify": "^3.2.3",
    "fuse.js": "^7.0.0",
    "@iconify-json/simple-icons": "^1.2.23",
    "@iconify-json/flat-color-icons": "^1.2.1"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/semver": "catalog:studiocms",
    "@types/mdast": "catalog:studiocms-shared",
    "@types/lodash": "catalog:studiocms-shared",
    "@types/figlet": "^1.7.0",
    "@types/node": "catalog:",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/diff": "^7.0.1",
    "@types/react-dom": "catalog:studiocms-renderer",
    "@types/react": "catalog:studiocms-renderer"
  },
  "peerDependencies": {
    "@astrojs/db": "catalog:min",
    "@astrojs/markdown-remark": "catalog:min",
    "@astrojs/react": "catalog:",
    "@astrojs/web-vitals": "catalog:",
    "astro": "catalog:min",
    "vite": "catalog:min"
  },
  "peerDependenciesMeta": {
    "@astrojs/react": {
      "optional": true
    },
    "@astrojs/web-vitals": {
      "optional": true
    }
  }
}
