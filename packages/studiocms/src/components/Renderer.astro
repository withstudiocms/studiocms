---
import contentRenderer from 'studiocms:renderer/current';

interface Props {
	content: string;
}

const { content } = Astro.props;

console.log('STARTING RENDERER');
// @ts-expect-error - `$$result` is a variable injected by the Astro compiler
const renderedContent = await contentRenderer(content, $$result);
---
<Fragment set:html={renderedContent}></Fragment>