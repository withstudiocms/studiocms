{
  "name": "@studiocms/renderers",
  "version": "0.1.0-beta.7",
  "description": "Renderers component for the StudioCMS Integration",
  "author": {
    "name": "Adam Matthiesen | Jacob Jenkins | Paul Valladares",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92"
  ],
  "license": "MIT",
  "keywords": [
    "astro-studiocms",
    "cms",
    "studiocms"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "build-scripts build 'src/**/*.ts' --build-tsconfig",
    "dev": "build-scripts dev 'src/**/*.ts' --build-tsconfig"
  },
  "files": [
    "dist", "components", "exports"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./contentrenderer": {
      "types": "./dist/lib/contentRenderer.d.ts",
      "default": "./dist/lib/contentRenderer.js"
    },
    "./components/Renderer": {
      "types": "./components/Renderer.d.ts",
      "default": "./components/Renderer.js"
    },
    "./exports": "./exports/index.ts",
    "./exports/markdoc/react": "./exports/markdocRenderers/markdocReact.ts"
  },
  "type": "module",
  "dependencies": {
    "@studiocms/core": "workspace:*",

    "astro-integration-kit": "catalog:",
    
    "@inox-tools/runtime-logger": "catalog:studiocms-shared",
    "@matthiesenxyz/astrodtsbuilder": "catalog:studiocms-shared",
    "@matthiesenxyz/integration-utils": "catalog:studiocms-shared",
    
    "@markdoc/markdoc": "catalog:studiocms-renderer",
    "@mdx-js/mdx": "catalog:studiocms-renderer",
    "react-dom": "catalog:studiocms-renderer",
    "react": "catalog:studiocms-renderer",
    "remark-gfm": "catalog:studiocms-renderer",
    "rehype-highlight": "catalog:studiocms-renderer",
    "unified": "catalog:studiocms-core"
  },
  "peerDependencies": {
    "@astrojs/react": "catalog:",
    "@astrojs/markdown-remark": "catalog:min",
    "astro": "catalog:min",
    "vite": "catalog:min"
  },
  "peerDependenciesMeta": {
    "@astrojs/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react-dom": "catalog:studiocms-renderer",
    "@types/react": "catalog:studiocms-renderer",
    "typescript": "catalog:"
  }
}
