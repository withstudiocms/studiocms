{
  "name": "@studiocms/dashboard",
  "version": "0.1.0-beta.7",
  "description": "Dashboard Integration for the StudioCMS Integration",
  "author": {
    "name": "Adam Matthiesen | Jacob Jenkins | Paul Valladares",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92"
  ],
  "license": "MIT",
  "keywords": [
    "astro-studiocms",
    "cms",
    "studiocms"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "src", "assets"
  ],
  "scripts": {
    "build": "build-scripts build 'src/**/*.ts' --build-tsconfig",
    "dev": "build-scripts dev 'src/**/*.ts' --build-tsconfig"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./components": "./assets/components/index.ts"
  },
  "type": "module",
  "dependencies": {
    "@studiocms/ui": "catalog:",
    "@studiocms/assets": "workspace:*",
    "@studiocms/betaresources": "workspace:*",
    "@studiocms/core": "workspace:*",
    "astro-integration-kit": "catalog:",
    "@inox-tools/runtime-logger": "catalog:studiocms-shared",
    "@matthiesenxyz/astrodtsbuilder": "catalog:studiocms-shared",
    "@matthiesenxyz/integration-utils": "catalog:studiocms-shared",
    "@fontsource-variable/onest": "catalog:studiocms-shared",
    "semver": "catalog:studiocms",
    "ultrahtml": "^1.5.3",
    "dompurify": "^3.2.3",
    "fuse.js": "^7.0.0"
  },
  "peerDependencies": {
    "astro": "catalog:min",
    "@astrojs/db": "catalog:",
    "@astrojs/web-vitals": "catalog:",
    "vite": "catalog:min"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/semver": "catalog:studiocms"
  }
}
