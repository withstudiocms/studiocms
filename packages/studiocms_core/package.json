    "./lib": "./src/lib/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./schemas/renderer": "./src/schemas/config/rendererConfig.ts",
    "./utils": "./src/utils/index.ts",
    "./consts": "./src/consts.ts",
    "./db/config": "./src/db/config.ts",
    "./db/tables": "./src/db/tables.ts",
    "./db/tsTables": "./src/db/tsTables.ts",
    "./sdk-utils": "./src/sdk-utils/index.ts",
    "./sdk-utils/tables": "./src/sdk-utils/tables.ts",
    "./sdk-utils/types": "./src/sdk-utils/types/index.ts",
    "./sdk-utils/utils": "./src/sdk-utils/utils.ts",
    "./sdk-utils/auth": "./src/sdk-utils/auth.ts",
    "./sdk-utils/delete": "./src/sdk-utils/delete.ts",
