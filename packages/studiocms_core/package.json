{
  "name": "@studiocms/core",
  "version": "0.1.0-beta.7",
  "description": "Core component for the StudioCMS Integration",
  "author": {
    "name": "Adam Matthiesen | Jacob Jenkins | Paul Valladares",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92"
  ],
  "license": "MIT",
  "keywords": [
    "astro-studiocms",
    "cms",
    "studiocms"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "build-scripts build 'src/**/*.ts' --build-tsconfig",
    "dev": "build-scripts dev 'src/**/*.ts' --build-tsconfig"
  },
  "sideEffects": false,
  "files": [
    "src", "assets"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": "./assets/components/index.ts",
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "default": "./dist/lib/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "default": "./dist/schemas/index.js"
    },
    "./schemas/renderer": {
      "types": "./dist/schemas/config/rendererConfig.d.ts",
      "default": "./dist/schemas/config/rendererConfig.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./consts": {
      "types": "./dist/consts.d.ts",
      "default": "./dist/consts.js"
    },
    "./db/config": {
      "types": "./dist/db/config.d.ts",
      "default": "./dist/db/config.js"
    },
    "./db/tables": "./src/db/tables.js",
    "./sdk-utils": {
      "types": "./dist/sdk-utils/index.d.ts",
      "default": "./dist/sdk-utils/index.js"
    },
    "./sdk-utils/tables": {
      "types": "./dist/sdk-utils/tables.d.ts",
      "default": "./dist/sdk-utils/tables.js"
    },
    "./sdk-utils/types": {
      "types": "./dist/sdk-utils/types.d.ts",
      "default": "./dist/sdk-utils/types.js"
    },
    "./sdk-utils/utils": {
      "types": "./dist/sdk-utils/utils.d.ts",
      "default": "./dist/sdk-utils/utils.js"
    },
    "./sdk-utils/auth": {
      "types": "./dist/sdk-utils/auth.d.ts",
      "default": "./dist/sdk-utils/auth.js"
    },
    "./sdk-utils/delete": {
      "types": "./dist/sdk-utils/delete.d.ts",
      "default": "./dist/sdk-utils/delete.js"
    },
    "./sdk-utils/get": {
      "types": "./dist/sdk-utils/get.d.ts",
      "default": "./dist/sdk-utils/get.js"
    },
    "./sdk-utils/post": {
      "types": "./dist/sdk-utils/post.d.ts",
      "default": "./dist/sdk-utils/post.js"
    },
    "./sdk-utils/update": {
      "types": "./dist/sdk-utils/update.d.ts",
      "default": "./dist/sdk-utils/update.js"
    },
    "./sdk-utils/init": {
      "types": "./dist/sdk-utils/init.d.ts",
      "default": "./dist/sdk-utils/init.js"
    }
  },
  "type": "module",
  "dependencies": {
    "@studiocms/ui": "catalog:",
    "@studiocms/robotstxt": "workspace:*",
    "astro-integration-kit": "catalog:",
    "@markdoc/markdoc": "catalog:studiocms-renderer",
    "mrmime": "catalog:studiocms-core",
    "unified": "catalog:studiocms-core",
    "remark-rehype": "catalog:studiocms-core",
    "mdast-util-to-hast": "catalog:studiocms-core",
    "rollup-plugin-copy": "catalog:studiocms-shared",
    "@matthiesenxyz/astrodtsbuilder": "catalog:studiocms-shared",
    "drizzle-orm": "catalog:studiocms-core",
    "lodash": "catalog:studiocms-shared",
    "mdast": "catalog:studiocms-shared",
    "mdast-util-from-markdown": "catalog:studiocms-shared",
    "mdast-util-to-markdown": "catalog:studiocms-shared",
    "mdast-util-to-string": "catalog:studiocms-shared",
    "unist-util-visit": "catalog:studiocms-shared"
  },
  "peerDependencies": {
    "@astrojs/db": "catalog:min",
    "astro": "catalog:min",
    "vite": "catalog:min"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/lodash": "catalog:studiocms-shared",
    "@types/mdast": "catalog:studiocms-shared"
  }
}
