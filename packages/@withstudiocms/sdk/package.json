{
  "name": "@withstudiocms/sdk",
  "version": "0.1.0-beta.0",
  "description": "StudioCMS Database SDK",
  "author": {
    "name": "withstudiocms",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git",
    "directory": "packages/@withstudiocms/sdk"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92",
    "code.spirit"
  ],
  "license": "MIT",
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm buildkit build 'src/**/*.{ts,json}'",
    "dev": "pnpm buildkit dev 'src/**/*.{ts,json}'",
    "typecheck": "tspc -p tsconfig.tspc.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "default": "./dist/context.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./*": "./dist/*"
  },
  "type": "module",
  "dependencies": {
		"diff": "^8.0.2",
		"diff2html": "^3.4.52",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "catalog:"
  },
  "peerDependencies": {
		"@withstudiocms/effect": "workspace:*",
    "@withstudiocms/kysely": "workspace:*"
  }
}
