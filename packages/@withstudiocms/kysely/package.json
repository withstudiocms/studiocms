{
  "name": "@withstudiocms/kysely",
  "version": "0.1.0",
  "description": "Type-safe database client and migration system for StudioCMS, built on top of Kysely",
  "author": {
    "name": "withstudiocms",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git",
    "directory": "packages/@withstudiocms/kysely"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92",
    "code.spirit"
  ],
  "license": "MIT",
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm buildkit build 'src/**/*.{ts,json}'",
    "dev": "pnpm buildkit dev 'src/**/*.{ts,json}'",
    "typecheck": "tspc -p tsconfig.tspc.json",
    "effect-check": "pnpm effect-language-service diagnostics --project tsconfig.tspc.json",
    "ci:effect-check": "pnpm effect-check --format github-actions",
    "test": "vitest"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./kysely": {
      "types": "./dist/kysely.d.ts",
      "default": "./dist/kysely.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./dist/schema.js"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.ts",
      "default": "./dist/test-utils.js"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "default": "./dist/core/*.js"
    },
    "./drivers/*": {
      "types": "./dist/drivers/*.d.ts",
      "default": "./dist/drivers/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js"
    }
  },
  "type": "module",
  "dependencies": {
    "kysely": "catalog:kysely",
    "kysely-turso": "catalog:kysely"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/pg": "catalog:kysely"
  },
  "peerDependencies": {
    "@libsql/client": "catalog:kysely",
    "effect": "catalog:effect",
    "kysely-turso": "catalog:kysely",
    "mysql2": "catalog:kysely",
    "pg": "catalog:kysely"
  },
  "peerDependenciesMeta": {
    "@libsql/client": {
      "optional": true
    },
    "kysely-turso": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  }
}
