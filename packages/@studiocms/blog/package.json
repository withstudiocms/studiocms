    "astro-studio-blog",
    "studiocms-blog",
    "plugin",
    "studiocms-plugin"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "buildkit build 'src/**/*.{ts,astro,css,json,png,d.ts}'",
    "dev": "buildkit dev 'src/**/*.{ts,astro,css,json,png,d.ts}'",
