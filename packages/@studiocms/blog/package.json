  "description": "Add a blog to your StudioCMS project with ease!",
  "author": {
    "name": "withstudiocms",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git",
    "directory": "packages/@studiocms/blog"
