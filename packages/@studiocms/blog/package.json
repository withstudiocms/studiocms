    "code.spirit"
  ],
  "license": "MIT",
  "keywords": [
    "astro",
    "astrocms",
    "astrodb",
    "astrostudio",
    "astro-studio",
    "astro-studiocms",
    "cms",
    "studiocms",
    "blog",
    "studio-blog",
