{
  "name": "@studiocms/md",
  "version": "0.1.1",
  "description": "Add Markdown Support to your StudioCMS project with ease!",
  "author": {
    "name": "withstudiocms",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git",
    "directory": "packages/@studiocms/md"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92",
    "code.spirit"
  ],
  "license": "MIT",
  "keywords": [
    "astro",
    "astrocms",
    "astrodb",
    "astrostudio",
    "astro-studio",
    "astro-studiocms",
    "cms",
    "studiocms",
    "plugin",
    "studiocms-plugin"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "buildkit build 'src/**/*.{ts,astro,css,json,png,d.ts}'",
    "dev": "buildkit dev 'src/**/*.{ts,astro,css,json,png,d.ts}'",
    "test": "vitest",
    "effect-check": "pnpm effect-language-service diagnostics --project tsconfig.tspc.json",
    "ci:effect-check": "pnpm effect-check --format github-actions",
    "typecheck": "tspc -p tsconfig.tspc.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./v/types": {
      "types": "./dist/virtual.d.ts",
      "default": "./dist/virtual.d.js"
    },
    "./editor": "./dist/components/markdown-editor.astro",
    "./renderer": {
      "types": "./dist/components/render.d.ts",
      "default": "./dist/components/render.js"
    }
  },
  "type": "module",
  "dependencies": {
    "@studiocms/markdown-remark-processor": "catalog:studiocms",
    "astro-integration-kit": "catalog:"
  },
  "devDependencies": {
    "@astrojs/markdown-remark": "catalog:min",
    "@types/node": "catalog:",
    "astro": "catalog:min",
    "studiocms": "workspace:^",
    "vite": "catalog:min"
  },
  "peerDependencies": {
    "effect": "catalog:effect"
  }
}
