{
	"name": "@studiocms/upgrade",
	"version": "0.2.1",
	"description": "StudioCMS CLI Package Upgrade Utility",
	"type": "module",
	"author": "withstudiocms",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/withstudiocms/studiocms.git",
		"directory": "packages/@studiocms/upgrade"
	},
	"bugs": "https://github.com/withstudiocms/studiocms/issues",
	"homepage": "https://studiocms.dev",
	"scripts": {
		"build": "tsdown",
		"cli:test": "pnpm build && node ./upgrade.mjs",
		"effect-check": "pnpm effect-language-service diagnostics --project tsconfig.tspc.json",
		"ci:effect-check": "pnpm effect-check --format github-actions",
		"typecheck": "tspc -p tsconfig.tspc.json"
	},
	"bin": {
		"@studiocms/upgrade": "./upgrade.mjs",
		"studiocms-upgrade": "./upgrade.mjs"
	},
	"files": [
		"upgrade.mjs",
		"dist"
	],
	"devDependencies": {
		"@types/semver": "catalog:",
		"@withstudiocms/cli-kit": "workspace:^",
		"@withstudiocms/effect": "workspace:^",
		"@withstudiocms/internal_helpers": "workspace:^",
		"dotenv": "catalog:",
		"effect": "catalog:effect",
		"package-manager-detector": "catalog:",
		"semver": "catalog:",
		"terminal-link": "^5.0.0",
		"tsdown": "catalog:"
	},
	"engines": {
		"node": "^20.3.0 || >=22.0.0"
	},
	"publishConfig": {
		"provenance": true
	}
}