{
  "name": "@studiocms/mdx",
  "version": "0.1.0-beta.28",
  "description": "Add MDX Support to your StudioCMS project with ease!",
  "author": {
    "name": "withstudiocms",
    "url": "https://studiocms.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withstudiocms/studiocms.git",
    "directory": "packages/@studiocms/mdx"
  },
  "contributors": [
    "Adammatthiesen",
    "jdtjenkins",
    "dreyfus92",
    "code.spirit"
  ],
  "license": "MIT",
  "keywords": [
    "astro",
    "astrocms",
    "astrodb",
    "astrostudio",
    "astro-studio",
    "astro-studiocms",
    "cms",
    "studiocms",
    "plugin",
    "studiocms-plugin"
  ],
  "homepage": "https://studiocms.dev",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "buildkit build 'src/**/*.{ts,astro,css,json,png,d.ts}'",
    "dev": "buildkit dev 'src/**/*.{ts,astro,css,json,png,d.ts}'",
    "test": "vitest",
    "typecheck": "tspc -p tsconfig.tspc.json"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./v/types": {
      "types": "./dist/virtual.d.ts",
      "default": "./dist/virtual.d.js"
    }
  },
  "type": "module",
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "astro-integration-kit": "catalog:",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react"
  },
  "peerDependencies": {
    "@studiocms/md": "workspace:*",
    "astro": "catalog:min",
    "effect": "catalog:effect",
    "studiocms": "workspace:*",
    "vite": "catalog:min"
  }
}
